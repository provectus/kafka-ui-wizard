    "deploy": "gh-pages --dist build --dotfiles true"
  },
  "devDependencies": {
    "@felte/validator-yup": "^1.0.8",
    "@sveltejs/adapter-static": "^1.0.0-next.31",
    "@sveltejs/kit": "1.0.0-next.350",
    "@types/gh-pages": "^3.2.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.7",
    "daisyui": "^2.14.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "felte": "^1.2.2",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.13",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "short-unique-id": "^4.4.4",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2",
    "yup": "^0.32.11"
